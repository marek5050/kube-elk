{
  "apiVersion": "v1",
  "kind": "PersistentVolume",
  "metadata": {
    "name": "pv0001"
  },
  "spec": {
    "capacity": {
      "storage": "5Gi"
    },
    "accessModes": [
      "ReadWriteOnce"
    ],
    "nfs": {
      "path": "/nfsShare",
      "server": "10.0.0.113"
    },
    "persistentVolumeReclaimPolicy": "Recycle"
  }
}